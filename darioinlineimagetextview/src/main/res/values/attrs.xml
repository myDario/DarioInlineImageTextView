<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="DarioInlineImageTextView">
        <attr name="inlineImgResourceId" format="integer"/>
        <attr name="inlineImgIndexInText" format="integer"/>
        <attr name="inlineImageVerticalAlign" format="enum">
            <enum name="bottom" value="0"/>
